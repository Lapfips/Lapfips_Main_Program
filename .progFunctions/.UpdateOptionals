#!/bin/bash

github="git@github.com:"
repositories_optional=(moi Lapfips.github.io PortoBlogProjectMetallica lapfips.github.io2)

for repo in "${repositories_optional[@]}"; do
    echo "updating $repo"
    ./GitRepositories/Lapfips_Main_Program/.progFunctions/.UpdateRepo "Lapfips/$repo" "$github"
done